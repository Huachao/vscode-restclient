{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["<", ">"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["<", ">"],
        ["\"", "\""]
    ],
    "folding": {
        "markers": {
            "start": "^(GET|POST|PUT|DELETE|PATCH|HEAD|OPTIONS|CONNECT|TRACE|LOCK|UNLOCK|PROPFIND|PROPPATCH|COPY|MOVE|MKCOL|MKCALENDAR|ACL|SEARCH|GRAPHQL|curl)\\s+",
            "end": "^#{3,}$"
        }
    }
}
